{
	"name":"fructify/theme",
	"type":"wordpress-theme",
	"description": "A WordPress theme with Laravel API Goodness :) ",
	"homepage": "https://github.com/fructify/theme",
	"keywords": ["fructify", "wordpress", "laravel"],
	"license": "MIT",
	"autoload":
	{
		"psr-4":
		{
			"Fructify\\": "./classes"
		}
	},
	"require":
	{
		"composer/installers": "*",
		"gears/blade": "*",
		"gears/router": "*",
		"gears/session": "*",
		"gears/assetmini": "*"
	},
	"extra":
	{
		"installer-name": "fructify",
		"assetmini-dir": "./theme-assets"
	}
}
