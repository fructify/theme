{
	"name":"fructify/theme",
	"type":"wordpress-theme",
	"description": "A WordPress theme with Modern PHP Goodness :)",
	"homepage": "https://github.com/fructify/theme",
	"keywords": ["fructify", "wordpress", "psr-7", "middleware"],
	"license": "MIT",
	"autoload":
	{
		"psr-4":
